CREATE TABLE trading_data (
    id SERIAL PRIMARY KEY,
    symbol VARCHAR(20),
    sectype VarChar(5),
    date DATE,
    time TIME,
    ask NUMERIC,
    ask_volume NUMERIC,
    bid NUMERIC,
    bid_volume NUMERIC,
    ask_time TIME,
    day_high_ask NUMERIC,  -- Ensure this matches exactly
    close NUMERIC,
    currency VARCHAR(10),
    day_high_ask_time TIME,
    day_high NUMERIC,
    isin VARCHAR(50),
    auction_price NUMERIC,
    day_low_ask NUMERIC,
    day_low NUMERIC,
    day_low_ask_time TIME,
    open NUMERIC,
    nominal_value NUMERIC,
    last NUMERIC,
    last_volume NUMERIC,
    trading_time TIME,
    total_volume NUMERIC,
    mid_price NUMERIC,
    trading_date DATE,
    profit NUMERIC,
    current_price NUMERIC,
    related_indices VARCHAR(255),
    day_high_bid_time TIME,
    day_low_bid_time TIME,
    open_time TIME,
    last_trade_time TIME,
    close_time TIME,
    day_high_time TIME,
    day_low_time TIME,
    bid_time TIME,
    auction_time TIME
);